{
    "name": "yesido49/scrapping-php",
    "description": "An exercise on scrapping with PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mehdi\\ScrappingPhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "YesIDo49",
            "email": "mehdialsidcheikh@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "symfony/http-client": "^6.3",
        "symfony/dom-crawler": "^6.3",
        "symfony/css-selector": "^6.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6||^10.2",
        "symfony/var-dumper": "^6.3"
    },
    "scripts": {
        "phpstan": "@php vendor/bin/phpstan analyse src --level=max",
        "test": "@php vendor/bin/phpunit tests"
    }
